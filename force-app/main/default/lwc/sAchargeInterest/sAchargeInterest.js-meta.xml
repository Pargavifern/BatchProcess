            <property name="interestAmount" type="integer" role="outputOnly"/>
            
            <property name="rate" type="string" role="inputOnly"/>
            
            <property name="interestBalance" type="integer" role="inputOnly"/>
            
            <property name="principalBalance" type="integer" role="inputOnly"/>
            
            <property name="lastChargedDate" type="Date" role="inputOnly"/>
            
