<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Maximum_and_Minimum_Amount_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>Maximum_Amount__c  &lt;=  Minimum_Amount__c</errorConditionFormula>
    <errorDisplayField>Maximum_Amount__c</errorDisplayField>
    <errorMessage>Maximum Amount must be greater than the Minimum Amount</errorMessage>
</ValidationRule>
