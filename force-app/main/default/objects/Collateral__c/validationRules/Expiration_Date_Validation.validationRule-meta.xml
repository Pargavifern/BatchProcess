<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expiration_Date_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>Issue_Date__c &gt;=  Expiration_Date__c</errorConditionFormula>
    <errorDisplayField>Expiration_Date__c</errorDisplayField>
    <errorMessage>Expiration must be after Issue Date</errorMessage>
</ValidationRule>
