<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>avoid_duplicate_name</fullName>
    <active>true</active>
    <errorConditionFormula>Name = VLOOKUP(
 $ObjectType.Savings_Product__c.Fields.Name ,  
 $ObjectType.Savings_Product__c.Fields.Name ,  
 Name)</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>This name is already exist. Please enter a unique name</errorMessage>
</ValidationRule>
