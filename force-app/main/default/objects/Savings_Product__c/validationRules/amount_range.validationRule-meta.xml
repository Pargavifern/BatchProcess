<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>amount_range</fullName>
    <active>true</active>
    <errorConditionFormula>Minimum_Amount__c &gt;  Maximum_Amount__c</errorConditionFormula>
    <errorDisplayField>Maximum_Amount__c</errorDisplayField>
    <errorMessage>Amount must be greater than Minimum Amount</errorMessage>
</ValidationRule>
