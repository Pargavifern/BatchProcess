<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cancel_reason_Valid_If_Not_canceled</fullName>
    <active>true</active>
    <errorConditionFormula>AND( NOT(ISPICKVAL( Cancel_Reason__c ,&quot;&quot; )), NOT( ISPICKVAL(Completion_Status__c ,&quot;Canceled&quot; )))</errorConditionFormula>
    <errorMessage>Cancel reason can only be changed by cancelling the application</errorMessage>
</ValidationRule>
