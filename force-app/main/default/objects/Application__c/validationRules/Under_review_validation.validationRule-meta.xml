<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Under_review_validation</fullName>
    <active>false</active>
    <errorConditionFormula>IF (ISPICKVAL(PRIORVALUE(Status__c),&quot;Submitted&quot;)
,ISPICKVAL(Status__c , &quot;Complete&quot;),false)</errorConditionFormula>
    <errorMessage>Status can only be changed to &apos;Under Review&apos; from the current status</errorMessage>
</ValidationRule>
